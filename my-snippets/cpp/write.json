{
  "Vector Output": {
    "prefix": "vout",
    "body": [
      "for (auto &val : ${1:arr}) {",
      "  cout << val << \" \";",
      "}",
      "cout << endl;",
      "$0"
    ],
    "description": "Output a 1D vector"
  },
  "Grid Output": {
    "prefix": "gout",
    "body": [
      "for (auto &${1:arr} : ${2:grid}) {",
      "  for (auto &val : $1) {",
      "    cout << val << \" \";",
      "  }",
      "  cout << endl;",
      "}",
      "cout << endl;",
      "$0"
    ],
    "description": "Output a 2D vector"
  },
  "Map Output": {
    "prefix": "mpout",
    "body": [
      "for (auto &[${1:key}, ${2:val}] : ${3:mp}) {",
      "  cout << $1 << \" --> \" << $2 << endl;",
      "}",
      "cout << endl;",
      "$0"
    ],
    "description": "Output a map with key, value pairs"
  }
}
